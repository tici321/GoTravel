package modelo;



public class Viagem {

	private int id;
	private String descricao;
	private double valor;
	private String ida;
	private String volta;
	
	public String getIda() {
		return ida;
	}

	public void setIda(String ida) {
		this.ida = ida;
	}

	public String getVolta() {
		return volta;
	}

	public void setVolta(String volta) {
		this.volta = volta;
	}

	public Viagem(int id, String descricao, double valor) {
	this.id=id;
	this.descricao=descricao;
	this.valor=valor;
	
	}

public Viagem() {
	
}

public int getId() {
	return id;
}

public void setId(int id) {
	this.id = id;
}

public String getDescricao() {
	return descricao;
}

public void setDescricao(String descricao) {
	this.descricao = descricao;
}

public double getValor() {
	return valor;
}

public void setValor(double valor) {
	this.valor = valor;
}

public void atualizarValor(double itens) {
	valor=valor*itens;
	
}

public static String getViagem() {
	// TODO Auto-generated method stub
	return null;
}
	
}
