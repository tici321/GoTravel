package modelo;

public class ItemPedido {

	private int id;
	private int itens;
	private double valor;
	private Viagem viagem;
	
	public ItemPedido(int id, int itens, double valor, Viagem viagem) {
		
		this.id=id;
		this.itens=itens;
		this.valor=valor;
		this.viagem=viagem;
	}
	
	public ItemPedido() {
}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getItens() {
		return itens;
	}

	public void setItens(int itens) {
		this.itens = itens;
	}

	public double getValor() {
		return valor;
	}

	public void setValor(double valor) {
		this.valor = valor;
	}

	public Viagem getViagem() {
		return viagem;
	}

	public void setViagem(Viagem viagem) {
		this.viagem = viagem;
	}
	
	public void calcularItem() {
	
	}
}
